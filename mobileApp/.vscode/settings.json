{
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "files.exclude": {
        "**/.*": true,

        "**/__pycache__": true,

        "**/*.iml": false,
        "**/*.lock": false,
        "**/analysis*": false,
        "android/": false,
        "build/": false,
        "ios/": true,
        "linux/": true,
        "macos/": true,
        "test/": false,
        "web/": true,
        "windows/": true
    },
    "java.configuration.updateBuildConfiguration": "automatic",
}
