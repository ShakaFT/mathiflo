{
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "files.exclude": {
        "**/.*": true,
        "**/__pycache__": true,
        "**/*.iml": true,
        "**/*.lock": true,
        "build": true,
        "linux/": true,
        "macos/": true,
        "test/": true,
        "web/": true,
        "windows/": true
    },
    "python.formatting.provider": "black"
}
